<Window x:Class="MediaPlayer.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Network Protocol" Height="112" Width="411">
    <Grid>
        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,24,0,0" Name="textBox1" VerticalAlignment="Top" Width="304" />
        <Label Content="Please enter a network URL :" Height="28" HorizontalAlignment="Left" Name="label1" VerticalAlignment="Top" />
        <Button Content="Play" Height="23" HorizontalAlignment="Left" Margin="322,23,0,0" Name="PlayUrl" VerticalAlignment="Top" Width="55" Click="PlayUrl_Click" />
        <Label Content="It can take several minutes" Height="28" HorizontalAlignment="Left" Margin="117,45,0,0" Name="label2" VerticalAlignment="Top" Width="160" />
    </Grid>
</Window>
